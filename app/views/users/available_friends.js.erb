$('#result_users').empty();
$('#result_users').append("<hr>");
$('#result_users').append("<%= j(render partial: 'users/result_friend_search', collection: @friends, as: :friend) %>");

$('#result_users .add_friend_result_wraper .add_to_query').on('click', function(){
	$(this).parent().remove();

	$.ajax({
		type: "POST",
		url: '/friend_relations',
		data: { 'target_id': $(this).parent().find('.result_id').text() },
		success: function (){
		}
	});
});
